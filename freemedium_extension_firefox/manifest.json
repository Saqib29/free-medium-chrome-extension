{
    "manifest_version": 3,
    "name": "Free medium extension",
    "version": "1.0.0",
    "description": "Read the premium medium articles free",
    "host_permissions": ["https://medium.com/*"],
    "permissions": ["tabs", "notifications"],
    "background": {
      "scripts": ["scripts/script.js"]
    },
    "action": {
      "default_icon": { "128": "images/icon-128.png" }
    },
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-64.png",
      "128": "images/icon-128.png"
  }
 }
  